# BlindInsight AI 환경 변수 템플릿
# 실제 사용을 위해서는 이 파일을 .env로 복사하고 실제 값을 입력하세요

# ============================================================
# 필수 설정
# ============================================================

# OpenAI API 키 (https://platform.openai.com/api-keys 에서 발급)
OPENAI_API_KEY="your-openai-api-key-here"

# ============================================================
# AI 모델 설정 (선택)
# ============================================================

# 기본 에이전트 모델 (미설정시 gpt-4o-mini-2024-07-18 사용)
# DEFAULT_AGENT_MODEL="gpt-4o-mini-2024-07-18"

# Supervisor 및 도구 모델
# SUPERVISOR_MODEL="gpt-4o-mini-2024-07-18"
# AGENT_TOOL_MODEL="gpt-4o-mini-2024-07-18"

# 개별 에이전트 모델 (각 에이전트별 다른 모델 사용 가능)
# SALARY_BENEFITS_AGENT_MODEL="gpt-4o-mini-2024-07-18"
# COMPANY_CULTURE_AGENT_MODEL="gpt-4o-mini-2024-07-18"
# WORK_LIFE_BALANCE_AGENT_MODEL="gpt-4o-mini-2024-07-18"
# MANAGEMENT_AGENT_MODEL="gpt-4o-mini-2024-07-18"
# CAREER_GROWTH_AGENT_MODEL="gpt-4o-mini-2024-07-18"
# QUALITY_EVALUATOR_AGENT_MODEL="gpt-4o-mini-2024-07-18"

# ============================================================
# 검색 및 재랭킹 설정 (선택)
# ============================================================

# 검색 파라미터
# DEFAULT_SEARCH_K=10              # 기본 검색 결과 수
# MAX_SEARCH_K=50                  # 최대 검색 결과 수
# RERANK_SEARCH_MULTIPLIER=2       # 재랭킹시 검색 배수

# 앙상블 검색 가중치 (BM25 + 벡터 검색, 합계 1.0)
# ENSEMBLE_WEIGHT_BM25=0.3         # 키워드 검색 가중치
# ENSEMBLE_WEIGHT_VECTOR=0.7       # 의미 검색 가중치
# RELEVANCE_SCORE_THRESHOLD=0.3    # 최소 관련성 점수

# ============================================================
# 데이터 처리 설정 (선택)
# ============================================================

# 카테고리별 청크 처리 파라미터
# CATEGORY_CHUNK_SIZE=1            # 카테고리별 청크 크기
# CATEGORY_MAX_CHUNK_SIZE=2        # 최대 청크 크기
# CATEGORY_MIN_SENTENCE_LENGTH=5   # 최소 문장 길이

# 배치 처리 설정
# AI_BATCH_SIZE=30                 # 임베딩 배치 크기 (메모리 부족시 감소)
# MAX_CHUNK_LENGTH=300             # 최대 청크 길이
# EMBEDDING_MODEL="text-embedding-3-small"  # OpenAI 임베딩 모델

# ============================================================
# 데이터베이스 및 경로 설정 (선택)
# ============================================================

# 벡터 데이터베이스 경로
# VECTOR_DB_PATH="./data/embeddings"
# JSON_DATA_PATH="./data/vectordb"
# DATA_DIRECTORY="./data"

# 데이터베이스 URL
# DATABASE_URL="sqlite:///C:/blind/data/blindinsight.db"

# ============================================================
# 디버깅 및 로깅 (선택)
# ============================================================

# LangSmith 추적 (LangGraph 워크플로우 디버깅용)
# LANGSMITH_API_KEY="your-langsmith-api-key-here"
# LANGSMITH_TRACING="true"

# 로깅 레벨
# LOG_LEVEL="INFO"  # DEBUG, INFO, WARNING, ERROR

# ============================================================
# 기타 API 키들 (미래 기능용)
# ============================================================

# ANTHROPIC_API_KEY="your-anthropic-api-key-here"
# GOOGLE_API_KEY="your-google-api-key-here"
# PERPLEXITY_API_KEY="your-perplexity-api-key-here"